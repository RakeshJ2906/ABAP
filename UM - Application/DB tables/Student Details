@EndUserText.label : 'Student Details'
@AbapCatalog.enhancement.category : #NOT_EXTENSIBLE
@AbapCatalog.tableCategory : #TRANSPARENT
@AbapCatalog.deliveryClass : #A
@AbapCatalog.dataMaintenance : #RESTRICTED
define table ztb_student_info {

  key client         : abap.clnt not null;
  key class          : abap.char(2) not null;
  key rollno         : abap.char(2) not null;
  firstname          : abap.char(40);
  lastname           : abap.char(40);
  age                : abap.numc(2);
  gender             : abap.char(1);
  dob                : abap.dats;
  avgpercentage      : abap.char(3);
  grade              : abap.char(1);
  behaviorrating     : abap.numc(1);
  locallastchangedat : timestampl;

}
